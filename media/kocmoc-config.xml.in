<!--
 This is the main config file for kocmoc:
 All config values must reside inside the <properties> tag. The following
 properties are allowed:
	<string name="foo" value="asdf" />
	<float name="bar" value="42.123456789" />
	<bool name="bario" value="1" />

 the names should use hyphens not cammelCase.
-->

<?xml version="1.0" ?>
<properties>
	<bool name="fullscreen" value="0" />
	<flaot name="width" value="720" />
	<float name="height" value="432" />
	
	<string name="media-path" value="${KOCMOC_MEDIA_PATH}" />
</properties>